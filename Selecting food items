#include<stdio.h>
int main()
{
     int choice;
     printf("enter a choice:");
     scanf("%d", &choice);
     switch(choice)
     {
          case 1:
               printf("you have chosen:\nfrench fries-Rs.99");
               break;
          case 2:
               printf("you have chosen:\npizza-Rs.239");
               break;
          case 3:
               printf("you have chosen:\npasta-Rs.179");
               break;
          case 4:
               printf("you have chosen:\nsandwich-Rs.149");
               break;
          case 5:
               printf("you have chosen:\nburger-Rs.129");
               break;
          default:printf("invalid choice");
     }
     return 0;
}
             

             
       
            
